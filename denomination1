amount=650
five_hundred_notes=amount//500
rem_balance=amount%500
print("five_hundred_notes(500):",five_hundred_notes)

hundred_notes=rem_balance//100
rem_balance=rem_balance%100
print("hundred_notes(100):",hundred_notes)

fifty_notes=rem_balance//50
rem_balance=rem_balance%50
print("fifty_notes(50):",fifty_notes)

ten_notes=rem_balance//10
rem_balance=rem_balance%10
print("ten_notes(10):",ten_notes)
